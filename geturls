gospider -s https://domain.com -d 10 -a -w --blacklist ".(jpg|jpeg|gif|css|tif|tiff|png|ttf|woff|woff2|ico|pdf|woff2|svg|js)" | grep -oP "(http[s]?:\/\/)?((-)?[\w+\.]){1,}domain\.com.*" | grep "=" | qsreplace payload | anew | sort -u | urlprobe -c 1000 -t 01 | grep 200 | awk '{print $5}' | tee -a gospiderurls.txt
